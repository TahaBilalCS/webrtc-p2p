{
    "name": "webrtc-p2p",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "build": "ng build",
        "i18n": "npm run extract-i18n && npm run i18n-create-json",
        "watch": "ng build --watch --configuration development",
        "prod": "ng build --watch --configuration production",
        "extract-i18n": "ng extract-i18n",
        "i18n-create-json": "bin/xliff-to-json/xliff-to-json src/locale/ && mv src/locale/*.json src/assets/i18n/",
        "test": "ng test",
        "lint": "ng lint",
        "format": "prettier --write ."
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "^16.2.0",
        "@angular/common": "^16.2.0",
        "@angular/compiler": "^16.2.0",
        "@angular/core": "^16.2.0",
        "@angular/forms": "^16.2.0",
        "@angular/localize": "~16.2.10",
        "@angular/platform-browser": "^16.2.0",
        "@angular/platform-browser-dynamic": "^16.2.0",
        "@angular/router": "^16.2.0",
        "bootstrap-icons": "^1.11.1",
        "primeng": "^16.5.1",
        "rxjs": "~7.8.0",
        "tslib": "^2.3.0",
        "zone.js": "~0.13.0"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^16.2.6",
        "@angular-eslint/builder": "16.2.0",
        "@angular-eslint/eslint-plugin": "16.2.0",
        "@angular-eslint/eslint-plugin-template": "16.2.0",
        "@angular-eslint/schematics": "16.2.0",
        "@angular-eslint/template-parser": "16.2.0",
        "@angular/cli": "^16.2.6",
        "@angular/compiler-cli": "^16.2.0",
        "@types/jasmine": "~4.3.0",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.1",
        "jasmine-core": "~4.6.0",
        "karma": "~6.4.0",
        "karma-chrome-launcher": "~3.2.0",
        "karma-coverage": "~2.2.0",
        "karma-jasmine": "~5.1.0",
        "karma-jasmine-html-reporter": "~2.1.0",
        "ng-extract-i18n-merge": "^2.7.1",
        "prettier": "^3.0.3",
        "prettier-eslint": "^16.1.1",
        "typescript": "~5.1.3",
        "xliff": "^6.2.0"
    }
}
